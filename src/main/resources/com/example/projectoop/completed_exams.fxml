<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.projectoop.CompletedExams"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <VBox layoutX="36.0" layoutY="36.0" spacing="10.0">
            <Label text="Completed Exams" />
            <ListView fx:id="completedExamsListView" prefHeight="168.0" prefWidth="537.0" />
        </VBox>

        <HBox layoutX="36.0" layoutY="340.0" spacing="10.0">
            <Button fx:id="backButton" text="Back" onAction="#handleBack" />
            <Button fx:id="detailsButton" text="Details" onAction="#handleDetails" />
        </HBox>
    </children>
</AnchorPane>