<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projectoop.UserExams">
    <children>
        <ListView fx:id="userExamsList" layoutX="41.0" layoutY="50.0" prefHeight="300.0" prefWidth="400.0" />
        <Button fx:id="createExamButton" layoutX="500.0" layoutY="50.0" mnemonicParsing="false" onAction="#handleCreateExam" text="Create Exam" />
        <Button fx:id="checkExamButton" layoutX="500.0" layoutY="100.0" mnemonicParsing="false" onAction="#handleCheckExam" text="Check Exam" />
        <Button fx:id="logoutButton" layoutX="500.0" layoutY="300.0" mnemonicParsing="false" onAction="#handleBack" text="Back" />
    </children>
</AnchorPane>
